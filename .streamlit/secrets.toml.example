# Streamlit Secrets Configuration
# Copy this file to 'secrets.toml' and add your values
# DO NOT commit secrets.toml to git!

# Google Drive Model URL
# 1. Upload your model to Google Drive
# 2. Right-click → Share → Anyone with link can view
# 3. Copy the file ID from the share link
# Example: https://drive.google.com/file/d/FILE_ID_HERE/view?usp=sharing
[model]
google_drive_id = "YOUR_GOOGLE_DRIVE_FILE_ID_HERE"

# Alternative: Direct download URL
# model_url = "https://your-direct-download-url.com/model.h5"
