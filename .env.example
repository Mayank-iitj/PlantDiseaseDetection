# Example environment variables for deployment
# Copy this to .env for local development (don't commit .env!)

# Model settings
MODEL_PATH=models/best_model.h5
MODEL_URL=  # Optional: URL to download model from (Google Drive, etc.)

# Application settings
DEBUG=False
MAX_UPLOAD_SIZE=200
IMAGE_SIZE=224

# Optional: Google Drive file ID for model download
# GDRIVE_MODEL_ID=your_file_id_here

# Optional: Custom class labels path
# CLASS_LABELS_PATH=class_labels.txt

# Streamlit settings (usually set via config.toml)
# STREAMLIT_SERVER_PORT=8501
# STREAMLIT_SERVER_ADDRESS=0.0.0.0
